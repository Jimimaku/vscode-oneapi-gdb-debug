{
	"name": "oneapi-debug",
	"displayName": "oneapi-debug (temporary)",
	"description": "oneapi-debug (temporary)",
	"publisher": "intel-corporation",
	"version": "0.0.1",
	"license": "MIT",
	"icon": "media/oneapi-logo.png",
	"keywords": [
		"intel",
		"oneapi",
		"debug"
	],
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:workbench.action.tasks.runTask",
		"onDebug:cppdbg"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "intelOneAPI.launchConfigurator.generateLaunchJson",
				"title": "Intel oneAPI: Generate launch configurations"
			}
		],
		"taskDefinitions": [
			{
				"type": "toolProvider",
				"properties": {
					"test1": {
						"type": "string",
						"description": "Testing"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"lint": "eslint -c .eslintrc.js --ext .ts ./",
		"package": "vsce package --yarn",
		"ui-test": "extest setup-and-run -o ./src/test/ui/configs/settings.json --yarn out/test/ui/*.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.18",
		"@types/mkdirp": "^1.0.1",
		"@types/mocha": "^8.2.2",
		"@types/node": "^15.0.3",
		"@types/rimraf": "^3.0.0",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.24.0",
		"@typescript-eslint/parser": "^4.23.0",
		"chai": "^4.3.4",
		"eslint": "^7.26.0",
		"mocha": "^8.4.0",
		"typescript": "^4.2.4",
		"vsce": "^1.88.0",
		"vscode-extension-tester": "^4.0.3",
		"vscode-extension-tester-native": "^3.0.0",
		"vscode-test": "^1.5.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/intel/vscode-analyzers.git"
	}
}
